  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "clear": "expo r -c",
    "test": "jest --watchAll",
    "build:android": "npx react-native run-android --variant=release",
    "build:ios": "npx react-native run-ios --configuration Release",
    "build": "yarn build:ios && yarn build:android",
    "ar-login": "npx google-artifactregistry-auth --repo-config=.npmrc",
    "android:dev": "SET ENVFILE=.env.dev && yarn run android",
    "android:uat": "SET ENVFILE=.env.uat && yarn run android",
    "android:prod": "SET ENVFILE=.env.prd && yarn run android",
    "android:qal": "SET ENVFILE=.env.qal && yarn run android",
    "postinstall": "react-native setup-ios-permissions && pod-install"
  },
